# Revenix Windows Agent environment.
# Copy this file to agent.env and update values.

API_URL=http://YOUR-MAIN-SERVER:8000
REDIS_URL=redis://YOUR-MAIN-SERVER:6379

# Optional
REDIS_PASSWORD=
# Leave blank for automatic NIC detection by bootstrap-install.ps1
NETWORK_INTERFACE=
# "true" is recommended for better visibility
PROMISCUOUS_MODE=true
